{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/microsoft/(.*)",
      "destination": "/server.ts"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/server.ts"
    }
  ]
}
